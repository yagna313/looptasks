   //task6
   function findLength(input) {
  let count = 0;
  let a = 0;
  while (input[a] !== undefined) {
    count++;
    a++;
  }
  return count;
}

let input = "YourInputString";
let length = findLength(input);
console.log("length:", length);
alert(length);
   
